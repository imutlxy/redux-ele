{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        [
            "import",
            [
                {
                    "style": true,
                    "libraryName": "antd-mobile"
                }
            ]
        ],
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-async-to-generator",
        "transform-runtime"
    ],
    "env": {
        "build": {
            "optional": [
                "optimisation",
                "minification"
            ]
        }
    }
}
