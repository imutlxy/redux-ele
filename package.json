{
  "name": "@ud/umd",
  "version": "0.5.1",
  "description": "Universal Modle Designer",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.itt.space/ud/umd.git"
  },
  "keywords": [
    "react",
    "ES6",
    "babel",
    "antd",
    "react-redux"
  ],
  "scripts": {
    "clean": "rimraf dist/* lib/*",
    "upgrade": "rm -rf node_modules && npm install",
    "lint": "eslint ./**/* --ext '.js,.jsx'",
    "lintfix": "eslint ./**/* --fix --ext '.js,.jsx'",
    "test": "casperjs test ./test",
    "build": "webpack",
    "build:theme": "export UWD_THEME=light && npm run build && cp ./dist/index.css ./dist/index.light.css ; cp ./dist/index.css ./src/index.light.css ; export UWD_THEME=dark",
    "dist": "npm run build:theme & webpack --env=dist",
    "postdist": "copyfiles -f ./src/*.css ./dist/ && copyfiles -f ./src/Constants.js ./dist/ ",
    "start": "node server.js --env=dev",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "validate": "npm ls",
    "build:lib": "rimraf lib && babel -d ./lib src/umd/lib --source-maps --extensions .es6,.es,.jsx,.js --copy-files",
    "snapshot": "npm publish --registry=https://repository.itt.space/repository/npm-snapshot",
    "release": "npm publish --registry=https://repository.itt.space/repository/npm-hosted"
  },
  "author": "BONC-UD",
  "dependencies": {
    "@ud/react-contextmenu": "^2.5.1",
    "@ud/uwd": "^1.1.28",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "lodash": "^4.17.4",
    "react-codemirror": "^0.3.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "@ud/sass-vars-to-js": "^0.8.8",
    "autoprefixer": "latest",
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-plugin-import": "latest",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "latest",
    "casperjs": "^1.1.3",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "latest",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "esdoc": "^0.5.2",
    "eslint": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-react": "latest",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "latest",
    "glob": "^7.1.2",
    "html-loader": "latest",
    "html-webpack-plugin": "latest",
    "less": "latest",
    "less-loader": "latest",
    "lodash-webpack-plugin": "^0.11.4",
    "markdown-loader": "latest",
    "mocha": "^3.2.0",
    "node-sass": "latest",
    "open": "0.0.5",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "latest",
    "prop-types": "^15.5.8",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "sass-loader": "latest",
    "string-replace-loader": "^1.2.0",
    "style-loader": "latest",
    "url-loader": "latest",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
